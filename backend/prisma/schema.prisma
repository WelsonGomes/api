
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tbestado {
  id Int @id @default(autoincrement())
  nome String @db.VarChar(100) @unique
  uf String @db.VarChar(2) @unique
  pais String @db.VarChar(100)
// cidade tbcidade[]
//  cliente tbcliente[]
}

