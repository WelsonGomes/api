{
  "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
      "sourceMap": true,
      "target": "es6",
      "module": "commonjs",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}



